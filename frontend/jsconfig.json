{
  "compilerOptions": {
    "target": "es6", // or whichever version you're using
    "module": "commonjs", // or "esnext" if using ES modules
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "build"]
}
