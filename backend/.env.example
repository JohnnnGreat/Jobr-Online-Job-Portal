# Appwrite configuration
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=your_project_id_here
APPWRITE_PROJECT_KEY=your_project_key_here

# Google OAuth configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=your_google_redirect_uri_here

# Bucket ID for file storage
BUCKET_ID=your_bucket_id_here

# MongoDB connection URLs
MONGO_DATABASE_URL=mongodb://localhost:27017/ecommerce
DB_URL=mongodb+srv://your_username:your_password@your_cluster.mongodb.net/your_database?retryWrites=true&w=majority

# JWT secret token
JWT_TOKEN=your_jwt_secret_token_here
